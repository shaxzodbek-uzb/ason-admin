                "type": "object"
            },
            "ApiUserLoginRequest": {
                "title": "User login request",
                "description": "Request object for login user.",
                "properties": {
                    "email": {
                        "title": "Email",
                        "description": "User email address",
                        "type": "string",
                        "format": "email",
                        "example": "admin@ason.com"
                    },
                    "password": {
                        "title": "Password",
                        "description": "password",
                        "type": "string",
                        "format": "string",
